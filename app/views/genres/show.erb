<h1><%= @genre.name %></h1>

<ul>
<p>Songs</p>
<% @genre.songs.each do |song| %>
<li><a href="/songs/<%= song.slug %>"><%= song.name  %> - <%= song.artist.name %></a></li>
<% end %>
<p>Artists</p>
<% @genre.artists.each do |artist| %>
        <li><a href='/artists/<%= artist.slug %>'><%= artist.name %></a></li>
       <% end %>
</ul>